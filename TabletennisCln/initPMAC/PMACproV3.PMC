; 加入击球瞬间X，Z轴的方向调整
OPEN PROG 12 CLEAR
	RAPID
	ABS
	P3=1
	P62=600	
	
	WHILE(P3=1)
		IF(P4=1)
			I5211=(P31-20)*8388608/I10		; 对切球的延迟
			WHILE(I5211>0)
			END WHILE
			
			LINEAR
			INC
			TA(20)
			TS(7.5)
			F(P62)
			X20Z20
			P41=0
			P4=0
			RAPID
			ABS
		END IF
		
		IF(P4=0)
			X0Z0
		END IF
	END WHILE
	X0
CLOSE
